# Database Configuration
# PostgreSQL connection string (Neon Serverless recommended)
DATABASE_URL=postgresql://user:password@host:5432/database?sslmode=require

# Server Configuration (optional - for local development)
PORT=5000

# SendGrid Email Configuration (optional)
SENDGRID_API_KEY=your_sendgrid_api_key

# Session Secret (optional - for production use a secure random string)
SESSION_SECRET=your_session_secret_here

# Vercel Cron Secret (required for cron jobs on Vercel)
CRON_SECRET=your_cron_secret_here
